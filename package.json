{
  "name": "Warehouse Inventory Tracker",
  "productName": "warehouse_inventory_tracker",
  "version": "1.0.0",
  "description": "Warehouse Inventory Tracker, Tracks incoming/outgoing/current stocked items in warehouses",
  "main": "src/index.js",
  "scripts": {
    "install-all": "npm install && install-routify",
    "install-routify": "cd client_ui && npm install",
    "package-client": "cd client_ui && npm run build",
    "dev": "cd client_ui && npm run dev",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [
    "Electronjs", "Electron-Forge"
  ],
  "author": {
    "name": "adamg",
    "email": "v.adam.gonzales@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "dependencies": {
    "better-sqlite3": "^7.5.2",
    "electron-squirrel-startup": "^1.0.0",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "17.1.0"
  }
}
